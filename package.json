{
  "name": "lapisdb",
  "version": "0.2.3",
  "description": "A TypeScript database built on top of LevelDB - a fast and efficient C++ database.",
  "main": "dist/index.js",
  "scripts": {
    "test": "rimraf database && mkdir database && jest -i --verbose",
    "watch:build": "rimraf dist && webpack --watch",
    "watch:server": "nodemon \"./dist/bundle.js\" --watch \"./dist\"",
    "dev": "npm run watch:build",
    "dev:watch": "npm-run-all --parallel watch:server watch:build",
    "build": "rimraf dist && tsc",
    "dev:start": "node dist/bundle.js",
    "dev:benchmark": "tsc -p tsconfig.benchmark.json && node ./benchmark/benchmark",
    "prod:publish": "npm run build && npm publish"
  },
  "keywords": [
    "db",
    "file",
    "json"
  ],
  "homepage": "https://kekland.github.io/lapisdb",
  "bugs": {
    "url": "https://github.com/kekland/lapisdb/issues",
    "email": "kk.erzhan@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kekland/lapisdb.git"
  },
  "author": "kekland <kk.erzhan@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "class-transformer": "^0.2.0",
    "class-validator": "^0.9.1",
    "encoding-down": "^6.0.1",
    "leveldown": "^4.0.2",
    "levelup": "^4.0.0",
    "nanoid": "^2.0.1",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/encoding-down": "^5.0.0",
    "@types/jest": "^24.0.11",
    "@types/leveldown": "^4.0.0",
    "@types/levelup": "^3.1.0",
    "@types/nanoid": "^1.2.1",
    "@types/node": "^12.0.10",
    "jest": "^24.5.0",
    "lodash": "^4.17.11",
    "nodemon": "^1.18.10",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.3",
    "ts-jest": "^24.0.0",
    "ts-loader": "^5.3.3",
    "typedoc": "^0.14.2",
    "typedoc-webpack-plugin": "^1.1.4",
    "typescript": "^3.3.3333",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0"
  }
}
